{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    }
  ],
  "retry": {  
    "strategy": "fixedDelay",  
    "maxRetryCount": 0,  
    "delayInterval": "00:00:00"  
  }
}